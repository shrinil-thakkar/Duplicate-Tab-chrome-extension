{
    "manifest_version": 2,
    "name": "Duplicate Tab",
    "version": "0.1",
    "background":
    {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts":
    [{
        "matches":
        [
          "<all_urls>"
        ],
        "js": ["jquery-2.1.3.min.js", "content.js"]
    }],
    "browser_action":
    {
        "default_icon": "image.png"
    }
}